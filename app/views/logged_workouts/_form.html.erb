<%= form_for @logged_workout, :html => {:id => "LoggedWorkoutForm"} do |f| %>
  <% if @logged_workout.errors.any? %>
    <div id="error_explanation">
      <h2><%= pluralize(viewing.errors.count, "error") %> prohibited this viewing from being saved:</h2>

      <ul>
      <% @logged_workout.errors.full_messages.each do |message| %>
        <li><%= message %></li>
      <% end %>
      </ul>
    </div>
  <% end %>

  <div class="field form-group">
    <%= f.label :start_time %>
    <%= f.datetime_select :start_time%>
  </div>

  <div class="field form-group">
    <%= f.label :name %>
    
    <%= f.select :name, Workout.all.collect { |w| [w.name]}, include_blank: true %>
    
    
  </div>



  <div class="actions">
    <%= f.submit class: 'btn btn-danger btn-lg'%>
  </div>
  
<% end %>

